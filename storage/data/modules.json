[  // the root of the file
    {  // each module has its own object

        "id": "honeyJar",  // the id of the module

        "label": "Honey Jar",  // the label displayed on the website *

        "description": "Changes honey bottle to a jar of honey, and renames the item.",  // the description displayed on the website *

        "category":  // which category to put the module in on the pack picker. options are "aesthetic", "utility" and "fixes". defaults to "aesthetic".

        "author": "Beatso" // the author of the pack. will be displayed on the picker
        
        "iconType": "gif",  // [optional] the file extension of the module icon. defaults to "png" if not included. *

        "hidden": false,  // [optional] if this is set to true, this module will not be displayed on the website. used for invisible merged packs. defaults to false.

        "lang": true,  // [optional] whether or not this is a lang module: https://github.com/LittleImprovementsCustom/LittleImprovementsCustom/wiki/Lang-modules

        "merges": [  // [optional] the list of modules this should be merged with if they have conflicting files
            {
                "id": "connectedSmoothStone",  // the id of the mergeable module
                "mergeWith": "honeyJarPlusConnectedSmoothStone"  // the id of the pack to replace the two mergeable module with
            }
        ],

        "prefers": [  // [optional] a way to only use one module when a certain two are selected
            {
                "id": "obsidianEnderChest",  // the module to compare with                
                "delete": "honeyJar"  // the module to delete, either honeyJar or obsidianEnderChest
            }
        ],
        
        "incompatibilities": ["lecternBottom"]  // a list of modules that should not be able to be selected alongside this module
        
    }
]